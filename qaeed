	}else{
       		if(them==='dark'){
				style.innerHTML=":root{    --icon-color: #fff;--primary-color: #0f1415 !important;--secondry-color: #09262E;--active-color: #234550;--global-color: white;--card-bg: aliceblue;}";
		document.head.appendChild(style);

		}else{
				style.innerHTML=":root{    --icon-color: #044154;--primary-color: #f2f3fb;--secondry-color:rgba(118, 170, 191, 0.50);--active-color: #fff;--global-color: white;--card-bg: aliceblue;}";
		document.head.appendChild(style);
		}
	}
	function logi(){
					let them = localStorage.getItem('them');
		if(them==='dark'){
					localStorage.setItem('them','white');
				style.innerHTML=":root{    --icon-color: #fff;--primary-color: #0f1415 !important;--secondry-color: #09262E;--active-color: #234550;--global-color: white;--card-bg: aliceblue;}";

		}else{
				localStorage.setItem('them','dark')
				style.innerHTML=":root{    --icon-color: #044154;--primary-color: #f2f3fb;--secondry-color: rgba(118, 170, 191, 0.50);--active-color: #fff;--global-color: white;--card-bg: aliceblue;}";

		}
		document.head.appendChild(style);
}
			

</script>
<script>

		var div = document.createElement('div');
div.innerHTML=`  <div class="" style="
    margin: auto;
    width: 100%;
    height: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
">
<h1 style="
    text-align: center;
    font-size: 4rem;
    font-family: cairo;
    font-weight: bolder;
" class='text-center'>قائد</h1>
    <p style="
    font-size: 30px;
    font-weight: bolder;
    text-align: center;
">معك ...  <span class="typed-text" style="
    color:#fff;
    text-align: right;
    font-family: Cairo;
    font-style: normal;
    line-height: 55px;
    font-size: 30px !important;
    font-weight: bolder !important;
"></span><span class="cursor mycu" style='display:none;>&nbsp;</span></p>
  </div>`;
	div.style=`
    position: absolute;
    z-index: 2147483647;
    height: 100%;
    width: 100%;
    top: 0;
    right: 0;
    margin: auto;`
	setTimeout(()=>{	
	document.querySelectorAll('.swiper-slide.swiper-slide-duplicate[data-swiper-slide-index="2"]')[1].appendChild(div);
// 		document.querySelectorAll('.swiper-slide.swiper-slide-duplicate[data-swiper-slide-index="2"]')[2].appendChild(div);
	const typedTextSpan = document.querySelector(".typed-text");
const cursorSpan = document.querySelector(".cursor");
const textArray = ["اليوم", "غدا", "الي الابد"];
const typingDelay = 200;
const erasingDelay = 100;
const newTextDelay = 2000; // Delay between current and next text
let textArrayIndex = 0;
let charIndex = 0;

function type() {
  if (charIndex < textArray[textArrayIndex].length) {
    if(!cursorSpan.classList.contains("typing")) cursorSpan.classList.add("typing");
    typedTextSpan.textContent += textArray[textArrayIndex].charAt(charIndex);
    charIndex++;

    setTimeout(type, typingDelay);
  } 
  else {
    cursorSpan.classList.remove("typing");
  	setTimeout(erase, newTextDelay);
  }
}

function erase() {
	if (charIndex > 0) {
    if(!cursorSpan.classList.contains("typing")) cursorSpan.classList.add("typing");
    typedTextSpan.textContent = textArray[textArrayIndex].substring(0, charIndex-1);
    charIndex--;
    setTimeout(erase, erasingDelay);
  } 
  else {
    cursorSpan.classList.remove("typing");
    textArrayIndex++;
    if(textArrayIndex>=textArray.length) textArrayIndex=0;
    setTimeout(type, typingDelay + 1100);
  }
}
  if(textArray.length){ setTimeout(type, newTextDelay + 250)};


	},3500)

</script>
